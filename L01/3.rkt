        ((= n 16) (display "шестнадцать"))
        ((= n 17) (display "семнадцать"))
        ((= n 18) (display "восемьнадцать"))
        ((= n 19) (display "девятнадцать"))
        ((= n 100) (display "сто")) )
  (and (cond
        ((= a 2) (display "двадцать"))
        ((= a 3) (display "тридцать"))
