        ((= a 8) (display "восемьдесят"))
        ((= a 9) (display "девяносто")) )
        (cond
        ((= b 1) (display " один"))
        ((= b 2) (display " два"))
        ((= b 3) (display " три"))
        ((= b 4) (display " четыре"))
        ((= b 5) (display " пять"))
        ((= b 6) (display " шесть"))
        ((= b 7) (display " семь"))
        ((= b 8) (display " восемь"))
        ((= b 9) (display " девять")) ) ) ) )
