        ((= n 2) (display "два"))
        ((= n 3) (display "три"))
        ((= n 4) (display "четыре"))
        ((= n 5) (display "пять"))
        ((= n 6) (display "шесть"))
        ((= n 7) (display "семь"))
        ((= n 8) (display "восемь"))
